<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Record Rules -->
        <record id="membership_member_user_rule" model="ir.rule">
            <field name="name">Membership Member: User Access</field>
            <field name="model_id" ref="model_membership_member"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="membership_type_user_rule" model="ir.rule">
            <field name="name">Membership Type: User Access</field>
            <field name="model_id" ref="membership_card.model_membership_type"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="membership_card_user_rule" model="ir.rule">
            <field name="name">Membership Card: User Access</field>
            <field name="model_id" ref="membership_card.model_membership_card"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>

